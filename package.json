{
  "name": "plist.wasm",
  "version": "0.1.1",
  "description": "WebAssembly wrapper for libplist - parse and convert Apple Property Lists",
  "type": "module",
  "main": "dist/plist.mjs",
  "module": "dist/plist.mjs",
  "exports": {
    ".": {
      "import": "./dist/plist.mjs",
      "default": "./dist/plist.mjs"
    }
  },
  "files": [
    "dist/*.mjs",
    "dist/*.wasm"
  ],
  "scripts": {
    "test": "node --test test/test.mjs",
    "prepare": "./build.sh"
  },
  "keywords": [
    "plist",
    "property-list",
    "apple",
    "xml",
    "binary",
    "wasm",
    "webassembly",
    "libplist"
  ],
  "license": "LGPL-2.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChiChou/plist.wasm.git"
  },
  "bugs": {
    "url": "https://github.com/ChiChou/plist.wasm/issues"
  },
  "homepage": "https://github.com/ChiChou/plist.wasm#readme",
  "engines": {
    "node": ">=22.0.0"
  }
}
